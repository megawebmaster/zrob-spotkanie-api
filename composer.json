{
    "name": "megawebmaster/zrob-spotkanie-api",
    "version": "0.1.0",
    "description": "API of the meeting scheduling app.",
    "license": "MIT",
    "type": "project",
    "private": true,
    "require": {
        "php": ">=8.1",
        "ext-pdo_pgsql": "*",
        "laravel/lumen-framework": "^10.0",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "fakerphp/faker": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {},
    "minimum-stability": "dev",
    "prefer-stable": true
}
